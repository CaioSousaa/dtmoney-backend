generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id              Int @id @default(autoincrement())
  title           String
  amountInCents   Int
  category        String
  createdAt       String
  type            String

  @@map("transaction")
}
